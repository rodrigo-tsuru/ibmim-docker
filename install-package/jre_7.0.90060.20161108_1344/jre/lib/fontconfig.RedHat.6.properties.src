filename.IPAMincho=/usr/share/fonts/ipa-mincho/ipam.ttf
filename.Lucida_Sans_Regular=$JRE_LIB_FONTS/LucidaSansRegular.ttf
filename.Lucida_Bright_Regular=$JRE_LIB_FONTS/LucidaBrightRegular.ttf
filename.Lucida_Bright_Demibold=$JRE_LIB_FONTS/LucidaBrightDemiBold.ttf
