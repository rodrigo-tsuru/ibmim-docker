# Search Sequences

sequence.allfonts=latin-1
# encodings from Chinese locales
sequence.allfonts.GB18030=latin-1,chinese-cn-iso10646
sequence.allfonts.GB2312=latin-1,chinese-cn-iso10646
sequence.allfonts.GBK=latin-1,chinese-cn-iso10646
sequence.allfonts.UTF-8.zh.CN=latin-1,chinese-cn-iso10646
sequence.allfonts.Big5=latin-1,chinese-tw-iso10646
sequence.allfonts.EUC-TW=latin-1,chinese-tw-iso10646
sequence.allfonts.UTF-8.zh.TW=latin-1,chinese-tw-iso10646
sequence.allfonts.UTF-8.zh.HK=latin-1,chinese-hk-iso10646
# encodings from Japanese locales
sequence.allfonts.x-euc-jp-linux=latin-1,japanese-iso10646
sequence.allfonts.UTF-8.ja=latin-1,japanese-iso10646
# encodings from Korean locales
sequence.allfonts.EUC-KR=latin-1,korean-iso10646
sequence.allfonts.UTF-8.ko=latin-1,korean-iso10646

sequence.fallback=lucida,chinese-tw-iso10646,chinese-cn-iso10646,japanese-iso10646,korean-iso10646,zysong18030

# Exclusion Ranges

